{
  "name": "robopeer",
  "module": "main.ts",
  "type": "module",
  "scripts": {
    "new": "bun --bun run ./scripts/new.ts",
    "dev": "bun --bun run ./src/main.ts",
    "api-get": "bun --bun run ./scripts/openapi.ts",
    "build": "bun --bun build ./src/main.ts --outfile ./build/robopeer.js",
    "compile": "bun --bun build --compile --minify ./src/main.ts --outfile ./build/robopeer"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "openapi-typescript": "^6.7.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "itty-router": "^5.0.15",
    "openapi-fetch": "^0.9.3"
  }
}